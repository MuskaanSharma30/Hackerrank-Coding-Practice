class Solution {
public:
    bool isValidSudoku(vector<vector<char>>& board) {
       for(int i = 0;i<9;i++){
            for(int j = 0;j<9;j++){
                if(board[i][j] != '.'){
                    for(int k =i+1;k<9;k++){
                        if(board[k][j] == board[i][j]){
                            return(false);
                        }
                    }
                    for(int k =j+1;k<9;k++){
                        if(board[i][k] == board[i][j]){
                            return(false);
                        }
                    }
                    int it = i/3;
                    int jt = j/3;
                    for(int a = 3*it;a<3*it+3;a++){
                        for(int b = 3*jt;b<3*jt+3;b++){
                            if(a != i && b != j && board[a][b] == board[i][j]){
                                return(false);
                            }
                        }
                    }
                }
            }
        }
        return(true);
    }
};  
        
