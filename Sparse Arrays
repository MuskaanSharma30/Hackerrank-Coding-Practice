#include <bits/stdc++.h>
using namespace std;

int main()
{
   int n,q,i,j;
   string str[10000],query[10000];
   cin>>n;
   for(i=0;i<n;i++)
   {
       cin>>str[i];
   }
   cin>>q;
   for(i=0;i<q;i++)
   {
    cin>>query[i];
   }
   for(i=0;i<q;i++)
   {
    int c=0;
       for(j=0;j<n;j++)
       {
           if(query[i].compare(str[j])==0)
           {
               c++;
           }
           
       }cout<<c<<endl;
   }
 return 0;  
}
